#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    int max_h = 0;    
    int target = 0;   

    // game loop
    while (1) {
        max_h = 0;   
        target = 0;  
        
        for (int i = 0; i < 8; i++) {
            int mountain_h;
            cin >> mountain_h; cin.ignore();
            
            if(mountain_h > max_h){
                max_h = mountain_h;
                target = i;
            }
        }

        cout << target << endl;
    }
}
